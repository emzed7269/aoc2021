        "def day1data(filename: str) -> list:\n",
        "  with open(filename) as input :\n",
        "      input_list = input.read().strip().split(\"\\n\")\n",
        "      input_list = list(map(int,input_list))\n",
        "      return input_list\n"
      ],
      "execution_count": 120,
      "outputs": []
