    {
      "cell_type": "code",
      "metadata": {
        "id": "H8prquWEATBn"
      },
      "source": [
        "def thirdstar(puzzle : list) -> int:\n",
        "  crdnt = [0,0]\n",
        "  lnty = len(puzzle)\n",
        "  i = 0\n",
        "  while i < lnty:\n",
        "    mtype,mvalue = puzzle[i], int(puzzle[i+1])\n",
        "    if mtype == 'forward': crdnt[0] += mvalue \n",
        "    elif mtype == 'down': crdnt[1] += mvalue\n",
        "    elif mtype =='up': crdnt[1] -= mvalue\n",
        "    i += 2\n",
        "  return crdnt[0]*crdnt[1]"
      ],
      "execution_count": 214,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "-hY5SoUhI-RX",
        "outputId": "55f72619-35fa-405a-b12b-297a36ade596"
      },
      "source": [
        "thirdstar(day2puzzle)"
      ],
      "execution_count": 215,
