        "def secondstar(puzzle : list) -> int:\n",
        "  icr = 0\n",
        "  x = 0\n",
        "  ln = len(puzzle)\n",
        "  while x < ln-3:\n",
        "    if puzzle[x+3] > puzzle[x] :\n",
        "      icr +=1\n",
        "    x +=1\n",
        "  return icr"
      ],
