      "source": [
        "def fifthstar(puzzle : list):\n",
        "    ln = len(puzzle)\n",
        "    bits=[[n[i:i+1] for i in range(0,len(n),1)] for n in puzzle]\n",
        "    lnn = len(bits[0])\n",
        "    print(ln)\n",
        "    x = 0\n",
        "    gamma, epsilon=[], []\n",
        "    while x < lnn:\n",
        "      md = mode(int(bits[i][x]) for i in range(ln))\n",
        "      eps = abs(md-1)\n",
        "      epsilon.append(eps)\n",
        "      gamma.append(md)\n",
        "      x += 1\n",
        "    return gamma, epsilon\n",
        "\n",
        "\n"
      ],
      "execution_count": 286,
      "outputs": []
    },
