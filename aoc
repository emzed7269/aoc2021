      "source": [
        "def firststar(puzzle: list) -> int:\n",
        "  icr = 0\n",
        "  x = 0\n",
        "  ln = len(puzzle)\n",
        "  while x < ln:\n",
        "    if puzzle[x] > puzzle[x-1] :\n",
        "      icr +=1\n",
        "      print(x, end=\";\") ; print(puzzle[x], end=\";\") ; print(puzzle[x-1], end=\";\") ; print(icr)\n",
        "    x +=1\n",
        "  return icr\n",
        "\n"
      ],
      "execution_count": 122,
      "outputs": []
    },
