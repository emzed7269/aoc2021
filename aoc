        "def day2data(filename: str) -> list:\n",
        "  with open(filename) as input :\n",
        "      input_list = input.read().strip()\n",
        "      input_list = re.split('\\n| ', input_list)\n",
        "      input_list = list(map(str,input_list))    \n",
        "      return input_list"
      ],
