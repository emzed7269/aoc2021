      "cell_type": "code",
      "metadata": {
        "id": "0GmbhkqNTZQi"
      },
      "source": [
        "def fourthstar(puzzle : list) -> int:\n",
        "  crdnt = [0,0]\n",
        "  aim = 0\n",
        "  lnty = len(puzzle)\n",
        "  i = 0\n",
        "  while i < lnty:\n",
        "    mtype,mvalue = puzzle[i], int(puzzle[i+1])\n",
        "    if mtype == 'forward': \n",
        "      crdnt[0] += mvalue\n",
        "      crdnt[1] += mvalue * aim\n",
        "    elif mtype == 'down': aim += mvalue\n",
        "    elif mtype =='up': aim -= mvalue\n",
        "    i += 2\n",
        "  return crdnt[0]*crdnt[1]"
      ],
      "execution_count": 216,
      "outputs": []
